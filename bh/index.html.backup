    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }
        
        :root {
            --contrast: #222222;
            --contrast-2: #575760;
            --contrast-3: #b2b2be;
            --base: #f0f0f0;
            --base-2: #f7f8f9;
            --base-3: #ffffff;
            --accent: #1e73be;
        }
        
        html {
            overflow-x: hidden;
            width: 100%;
        }
        
        body {
            font-family: 'Oswald', sans-serif;
            font-weight: 300;
            font-size: 30px;
            background-color: var(--base-3);
            color: var(--contrast);
            line-height: 1.6;
            overflow-x: hidden;
            width: 100%;
            max-width: 100vw;
        }
        
        h1 {
            font-family: 'Bebas Neue', display;
            font-size: 70px;
            font-weight: normal;
            line-height: 1.2;
        }
        
        h2 {
            font-family: 'Bebas Neue', display;
            font-size: 100px;
            font-weight: normal;
            line-height: 1.2;
        }
        
        h3 {
            font-family: 'Bebas Neue', display;
            font-size: 50px;
            font-weight: normal;
            line-height: 1.2;
        }
        
        /* Touch optimization */
        a, button {
            -webkit-tap-highlight-color: rgba(0,0,0,0.1);
            touch-action: manipulation;
        }
        
        img {
            max-width: 100%;
            height: auto;
            display: block;
        }
        
        @media (max-width: 768px) {
            h1 { font-size: 50px; }
            h2 { font-size: 50px; }
            body { font-size: 20px; }
        }
        
        .grid-container {
            max-width: 1200px;
            margin: 0 auto;
            padding: 0 40px;
            width: 100%;
            box-sizing: border-box;
        }
        
        /* Header Section */
        .header-section {
            background: var(--contrast);
            padding: 15px 0;
        }
        
        .header-content {
            display: flex;
            justify-content: space-between;
            align-items: center;
            max-width: 1200px;
            margin: 0 auto;
            padding: 0 40px;
            width: 100%;
            box-sizing: border-box;
        }
        
        .header-logo img {
            height: 85px;
            width: auto;
        }
        
        .header-phones {
            display: flex;
            gap: 20px;
            align-items: center;
            font-size: 18px;
            flex-shrink: 0;
        }
        
        .header-phones a {
            color: var(--base-3);
            text-decoration: none;
            -webkit-tap-highlight-color: rgba(255,255,255,0.2);
            white-space: nowrap;
        }
        
        .header-phones a:hover {
            text-decoration: underline;
        }
        
        .header-separator {
            color: var(--base-3);
        }
        
        /* Hero Section */
        .hero-section {
            background: linear-gradient(135deg, rgba(255,255,255,0.95) 0%, rgba(240,248,252,0.95) 100%), 
                        url('https://www.brentmoelleken.com/wp-content/uploads/2024/05/hero-background.jpg') center/cover;
            padding: 80px 0;
            min-height: 85vh;
            display: flex;
            align-items: center;
            position: relative;
        }
        
        .hero-container {
            display: grid;
            grid-template-columns: 1fr 1fr;
            gap: 40px;
            align-items: center;
            max-width: 1200px;
            margin: 0 auto;
            padding: 0 40px;
            width: 100%;
            box-sizing: border-box;
        }
        
        .hero-left {
            text-align: left;
        }
        
        .hero-right {
            justify-self: end;
        }
        
        .hero-left h1 {
            margin-bottom: 15px;
            color: var(--contrast);
        }
        
        .hero-location {
            font-size: 28px;
            color: var(--contrast-2);
            margin-bottom: 30px;
            font-weight: 300;
        }
        
        .hero-image-container {
            text-align: center;
            margin: 30px 0;
        }
        
        .hero-image-container img {
            width: 211px;
            height: 211px;
            border-radius: 50%;
            box-shadow: 0 4px 20px rgba(0,0,0,0.15);
        }
        
        .hero-credentials {
            margin-top: 30px;
            font-size: 20px;
            line-height: 1.9;
            color: var(--contrast);
        }
        
        .hero-credentials strong {
            font-weight: 600;
            color: var(--contrast);
        }
        
        .university-logos {
            margin-top: 25px;
            display: flex;
            justify-content: center;
            align-items: center;
            gap: 40px;
            flex-wrap: wrap;
        }
        
        .university-logos img {
            max-width: 100%;
            height: auto;
            max-height: 45px;
            flex: 0 0 auto;
            opacity: 0.7;
        }
        
        .hero-right {
            position: relative;
            align-self: center;
        }
        
        .cta-box {
            background: var(--base-3);
            padding: 40px;
            box-shadow: 0 10px 40px rgba(0,0,0,0.15);
            border-radius: 3px;
        }
        
        .cta-box h3 {
            font-size: 42px;
            margin-bottom: 20px;
            color: var(--contrast);
        }
        
        .cta-box p {
            font-size: 22px;
            margin-bottom: 25px;
            line-height: 1.7;
            color: var(--contrast);
        }
        
        .phone-display {
            font-size: 36px;
            font-weight: 600;
            color: var(--accent);
            display: block;
            margin: 15px 0;
            text-decoration: none;
            font-family: 'Oswald', sans-serif;
            min-height: 44px;
            display: block;
            -webkit-tap-highlight-color: rgba(0,0,0,0.1);
        }
        
        .phone-display:hover {
            color: #155a94;
        }
        
        .call-cta {
            display: inline-block;
            background: var(--accent);
            color: var(--base-3);
            padding: 18px 45px;
            font-size: 26px;
            font-weight: 500;
            text-decoration: none;
            border-radius: 3px;
            margin-top: 20px;
            transition: background 0.3s;
            text-align: center;
            width: 100%;
            min-height: 44px;
            -webkit-tap-highlight-color: rgba(0,0,0,0.1);
        }
        
        .call-cta:hover {
            background: #155a94;
        }
        
        /* About Section */
        .about-section {
            padding: 80px 0;
            text-align: center;
            background: var(--base-3);
        }
        
        .about-section h2 {
            margin-bottom: 40px;
            color: var(--contrast);
        }
        
        .about-section p {
            font-size: 26px;
            max-width: 1100px;
            margin: 0 auto 50px;
            line-height: 1.8;
            color: var(--contrast);
            font-weight: 300;
        }
        
        .awards-logos {
            margin-top: 50px;
            text-align: center;
            display: flex;
            justify-content: center;
            align-items: center;
        }
        
        .awards-logos img {
            max-width: 100%;
            height: auto;
        }
        
        /* Before and After Section */
        .before-after-section {
            padding: 80px 0;
            background: var(--base-3);
            text-align: center;
        }
        
        .before-after-section h2 {
            margin-bottom: 15px;
            color: var(--contrast);
        }
        
        .before-after-section h3 {
            font-size: 24px;
            font-weight: 300;
            margin-bottom: 50px;
            color: var(--contrast-2);
            font-family: 'Oswald', sans-serif;
        }
        
        .before-after-gallery {
            display: grid;
            grid-template-columns: repeat(3, 1fr);
            gap: 30px;
            max-width: 1200px;
            margin: 0 auto;
            padding: 0 40px;
        }
        
        .before-after-item {
            width: 100%;
        }
        
        .before-after-item img {
            width: 100%;
            height: auto;
            border-radius: 3px;
            box-shadow: 0 4px 20px rgba(0,0,0,0.1);
        }
        
        /* Reviews Section */
        .reviews-section {
            padding: 80px 0;
            background: var(--base-3);
            text-align: center;
            display: block;
            visibility: visible;
            min-height: 500px;
        }
        
        .reviews-section h2 {
            margin-bottom: 15px;
            color: var(--contrast);
            display: block;
        }
        
        .reviews-section .reviews-subtitle {
            font-size: 20px;
            color: var(--contrast-2);
            margin-bottom: 50px;
            font-weight: 300;
            max-width: 800px;
            margin-left: auto;
            margin-right: auto;
        }
        
        .reviews-grid {
            display: grid;
            grid-template-columns: repeat(3, 1fr);
            gap: 30px;
            max-width: 1200px;
            margin: 0 auto;
            padding: 0 40px;
            visibility: visible;
        }
        
        .review-item {
            background: var(--base-2);
            padding: 40px;
            border-radius: 3px;
            text-align: left;
            box-shadow: 0 4px 15px rgba(0,0,0,0.08);
        }
        
        .review-item:nth-child(odd) {
            background: #eee;
        }
        
        .review-item h3 {
            font-size: 24px;
            font-weight: 600;
            color: var(--contrast);
            margin-bottom: 15px;
            line-height: 1.3;
            font-family: 'Oswald', sans-serif;
            text-transform: uppercase;
        }
        
        .review-stars {
            margin-bottom: 20px;
        }
        
        .review-stars img {
            width: 129px;
            height: 26px;
            display: block;
        }
        
        .review-date {
            font-size: 16px;
            color: var(--contrast-2);
            margin-bottom: 20px;
            font-weight: 300;
        }
        
        .review-item p {
            font-size: 18px;
            line-height: 1.8;
            color: var(--contrast);
            font-weight: 300;
            margin-bottom: 20px;
        }
        
        .review-author {
            font-size: 16px;
            color: var(--contrast);
            font-weight: 500;
            margin-top: 20px;
        }
        
        .review-author strong {
            font-weight: 600;
        }
        
        /* Team Section */
        .team-section {
            padding: 80px 0;
            background: var(--base-3);
        }
        
        .team-section h2 {
            text-align: center;
            margin-bottom: 50px;
            color: var(--contrast);
        }
        
        .team-image {
            text-align: center;
            margin-bottom: 60px;
        }
        
        .team-image img {
            max-width: 100%;
            height: auto;
            box-shadow: 0 4px 20px rgba(0,0,0,0.1);
        }
        
        .procedures-intro {
            text-align: center;
            margin-top: 80px;
        }
        
        .procedures-intro h2 {
            margin-bottom: 0;
        }
        
        /* Procedures Sections */
        .procedure-section {
            padding: 100px 0;
            min-height: 70vh;
            display: flex;
            align-items: center;
            position: relative;
            background-size: cover;
            background-position: center;
        }
        
        .procedure-section::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: inherit;
            z-index: 0;
        }
        
        .procedure-section.face-section {
            background: linear-gradient(135deg, rgba(232, 244, 248, 0.97) 0%, rgba(240, 248, 252, 0.97) 100%),
                        url('https://www.brentmoelleken.com/wp-content/uploads/2024/05/face-bg.jpg') center/cover;
        }
        
        .procedure-section.body-section {
            background: linear-gradient(135deg, rgba(245, 245, 240, 0.97) 0%, rgba(250, 250, 245, 0.97) 100%),
                        url('https://www.brentmoelleken.com/wp-content/uploads/2024/05/body-bg.jpg') center/cover;
        }
        
        .procedure-section.spa-section {
            background: linear-gradient(135deg, rgba(232, 244, 248, 0.97) 0%, rgba(240, 248, 252, 0.97) 100%),
                        url('https://www.brentmoelleken.com/wp-content/uploads/2024/05/spa-bg.jpg') center/cover;
        }
        
        .procedure-container {
            display: grid;
            grid-template-columns: 1fr 1fr;
            gap: 40px;
            align-items: start;
            max-width: 1200px;
            margin: 0 auto;
            padding: 0 40px;
            position: relative;
            z-index: 1;
            width: 100%;
            box-sizing: border-box;
        }
        
        .procedure-left {
            color: var(--contrast);
        }
        
        .procedure-left h2 {
            color: var(--contrast);
            margin-bottom: 35px;
        }
        
        .procedure-left p {
            font-size: 22px;
            line-height: 1.8;
            color: var(--contrast);
            font-weight: 300;
        }
        
        .procedure-right {
            background: var(--base-3);
            padding: 40px;
            box-shadow: 0 8px 30px rgba(0,0,0,0.12);
            border-radius: 3px;
        }
        
        .procedure-list {
            list-style: none;
            font-size: 20px;
            line-height: 1.8;
            margin-bottom: 30px;
        }
        
        .procedure-list p {
            font-weight: 600;
            margin-top: 25px;
            margin-bottom: 12px;
            font-size: 24px;
            color: var(--contrast);
        }
        
        .procedure-list p:first-child {
            margin-top: 0;
        }
        
        .procedure-list ul {
            list-style: none;
            padding-left: 0;
        }
        
        .procedure-list li {
            padding-left: 25px;
            position: relative;
            margin-bottom: 8px;
            color: var(--contrast-2);
        }
        
        .procedure-list li:before {
            content: "•";
            position: absolute;
            left: 0;
            color: var(--accent);
            font-weight: bold;
        }
        
        /* Footer Section */
        .footer-section {
            background: var(--contrast);
            padding: 80px 0 60px;
            color: var(--base-3);
        }
        
        .footer-container {
            display: grid;
            grid-template-columns: 1fr 1fr;
            gap: 60px;
            max-width: 1200px;
            margin: 0 auto;
            padding: 0 40px;
            width: 100%;
            box-sizing: border-box;
        }
        
        .footer-cta h3 {
            margin-bottom: 25px;
            color: var(--base-3);
            font-size: 42px;
        }
        
        .footer-cta p {
            color: var(--base-3);
            font-size: 22px;
            margin-bottom: 25px;
            line-height: 1.7;
        }
        
        .call-cta-white {
            background: var(--base-3);
            color: var(--contrast);
        }
        
        .call-cta-white:hover {
            background: var(--base-2);
        }
        
        .footer-info {
            justify-self: end;
        }
        
        .footer-logo {
            text-align: right;
            margin-bottom: 40px;
        }
        
        .footer-logo img {
            max-width: 350px;
            height: auto;
        }
        
        .footer-locations h2 {
            font-size: 50px;
            margin-bottom: 30px;
            color: var(--base-3);
            text-align: right;
        }
        
        .location {
            margin-bottom: 35px;
            font-size: 20px;
            line-height: 1.8;
            color: var(--base-3);
            text-align: right;
        }
        
        .location-phone {
            font-size: 26px;
            font-weight: 600;
            margin-top: 12px;
        }
        
        .location-phone a {
            color: var(--base-3);
            text-decoration: none;
            font-family: 'Oswald', sans-serif;
            -webkit-tap-highlight-color: rgba(255,255,255,0.2);
        }
        
        .location-phone a:hover {
            text-decoration: underline;
        }
        
        .footer-social {
            text-align: right;
            margin-top: 40px;
        }
        
        .footer-social h2 {
            font-size: 50px;
            margin-bottom: 25px;
            color: var(--base-3);
        }
        
        .social-icons {
            display: flex;
            justify-content: flex-end;
            gap: 20px;
        }
        
        .social-icons img {
            width: 47px;
            height: 47px;
            transition: opacity 0.3s;
        }
        
        .social-icons img:hover {
            opacity: 0.7;
        }
        
        /* Responsive */
        @media (max-width: 1024px) {
            .hero-container,
            .procedure-container,
            .footer-container {
                grid-template-columns: 1fr;
                gap: 40px;
            }
            
            .footer-info {
                justify-self: center;
            }
            
            .footer-logo,
            .footer-locations h2,
            .location,
            .footer-social {
                text-align: center;
            }
            
            .social-icons {
                justify-content: center;
            }
            
            .grid-container {
                padding: 0 30px;
            }
            
            .university-logos {
                gap: 30px;
                margin-top: 20px;
            }
            
            .university-logos img {
                max-height: 35px;
                opacity: 0.6;
            }
            
            .before-after-gallery {
                grid-template-columns: repeat(2, 1fr);
                gap: 20px;
                padding: 0 30px;
            }
            
            .before-after-section h2 {
                font-size: 48px;
                margin-bottom: 10px;
            }
            
            .before-after-section h3 {
                font-size: 20px;
                margin-bottom: 30px;
            }
            
            .reviews-grid {
                grid-template-columns: repeat(2, 1fr);
                gap: 25px;
                padding: 0 30px;
            }
            
            .reviews-section {
                padding: 60px 0;
            }
            
            .reviews-section .reviews-subtitle {
                font-size: 18px;
                margin-bottom: 40px;
            }
        }
        
